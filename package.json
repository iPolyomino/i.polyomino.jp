{
  "name": "polyomino.jp",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "iPolyomino <macbookpromacbookpromacbookpro@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "npm run generate-json && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "ava --serial --verbose",
    "generate-json": "processmd posts/**/*.md --outputDir posts/json",
    "summary": "mkdir -p posts/json && processmd posts/**/*.md --stdout --outputDir posts/json > posts/json/summary.json"
  },
  "dependencies": {
    "@nuxtjs/markdownit": "^1.2.0",
    "@nuxtjs/pwa": "^2.0.8",
    "axios": "^0.18.0",
    "nuxt": "^1.0.0",
    "processmd": "^4.2.1",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^0.4.1",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "jsdom": "^11.7.0",
    "prettier": "^1.14.2"
  }
}
